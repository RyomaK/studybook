<div id="main">
	<div class="container">
		<div class="row">
			<div id="content" class="8u skel-cell-important">
				<section>
					<header>
						<h2>得点記入</h2>
					</header>
					<form action = "/new" method ="POST">
						<input type = "number" name = "id" value=null>
						<div>
							学期:<input type = "number" name = "semester" value=<%=@semester%>>
						</div>
						<div>
							テスト名:<input type="text" name="test" value=<%=@test%> >
						</div>
						<div>
							学校名:<input type="text" name="school_name" value=<%=@school_name%>>
						</div>
						<div>
							得点:
							<table>
								<tr><td>国語<input type = "number" name = "japanese" value=<%=@japanese%>></td></tr>
								<tr><td>数学<input type = "number" name = "math" value=<%=@math%>></td></tr>
								<tr><td>英語<input type = "number" name = "english" value=<%=@english%>></td></tr>
								<tr><td>理科<input type = "number" name = "science" value=<%=@science%>></td></tr>
								<tr><td>社会<input type = "number" name = "social" value=<%=@social%>></td></tr>
							</table>
							<h1>これでよろしいですか？？</h1>
							<input type="submit" id="submit" value="OK">
						</form>
					</section>
				</div>
			</div>
		</div>
	</div>




